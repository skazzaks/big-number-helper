{
"manifest_version": 2,
    "name": "Big Number Helper!",
    "description": "Finally, grasp big numbers!",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ],
    "browser_action": {
        "default_icon": "img/32.png"
    },
    "icons": {
    "16": "img/16.png",
    "32": "img/32.png",
    "128": "img/128.png"
    }
}